page {
  background: #f5f5f6;
}
.navScroll {
  display: flex;
  justify-content: space-around;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1px solid #eee;
}
.navScroll .navItem {
  padding: 0 30rpx;
}

.active {
  border-bottom: 1px solid #f00;
}

/* 内容 */
.swiperContainer {
  height: 2000rpx;
}

/* 自习室座位预约 */
.listContainer {
  background: #fff;
  border-radius: 12rpx;
  margin: 20rpx 15rpx;
  padding: 15rpx 20rpx;
  font-size: 30rpx;
}
.item-pdg {
  padding-bottom: 15rpx;
}
.item-pdg view {
  display: inline-block;
}
.icon-more .more {
  float: right;
}
.info-btn {
  width: 200rpx;
  height: 50rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background: #05d405;
  text-align: center;
  line-height: 50rpx;
  color: #fff;
}
.text {
  width: 18%;
  text-align: justify;
  text-align-last: justify;
}

/* 教室预约列表 */

.zhuangtai {
  float: right;
  padding: 5rpx;
  border: 1px solid #ccc;
  color: #05db05;
  border-radius: 15rpx;
  font-size: 26rpx;
}
/* .classroomItem .ordertime {
  font-size: 28rpx;
} */

.class-state {
  width: 180rpx;
  height: 50rpx;
  margin: 0 auto;
  border: 2rpx solid #ccc;
  background-color: #eee;
  text-align: center;
  font-weight: 600;
  color: rgb(31, 33, 180);
}

/* 宿舍维修预约列表 */
.maintainItem .gzms {
  padding-bottom: 30rpx;
}
.maintainItem .bxtime {
  font-size: 28rpx;
}

